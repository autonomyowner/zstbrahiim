# Supabase Configuration
# Copy this file to .env.local and fill in your actual values from https://app.supabase.com

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Your Supabase anon/public key (safe to use in the browser)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Your Supabase service role key (keep this secret, server-side only)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Site URL for redirects (update this for production)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
